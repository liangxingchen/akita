{
  "name": "akita",
  "version": "0.12.0",
  "description": "akita api client",
  "main": "lib/node.js",
  "browser": "lib/client.js",
  "miniprogram": "lib",
  "types": "index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "cross-env DEBUG=akita* tape test/ts.js",
    "cover": "cross-env DEBUG=akita* nyc node_modules/.bin/tape test/ts.js; nyc report --reporter=html",
    "eslint": "eslint --ext .ts ./",
    "eslint:fix": "eslint --ext .ts --fix ./",
    "prettier": "prettier -l src/** test/**",
    "prettier:fix": "prettier --write -l src/** test/**",
    "fix": "yarn prettier:fix",
    "prepublish": "npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maichong/akita.git"
  },
  "author": {
    "name": "Liang",
    "email": "liang@maichong.it",
    "url": "https://github.com/liangxingchen"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maichong/akita/issues"
  },
  "homepage": "https://github.com/maichong/akita#readme",
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      ".history",
      "coverage",
      "test",
      "lib",
      "**/*.d.ts",
      "*.js"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "dependencies": {
    "debug": "^4.3.1",
    "form-data": "^4.0.0",
    "node-fetch-unix": "^2.3.0",
    "qs": "^6.10.1"
  },
  "devDependencies": {
    "@types/busboy": "^0.2.3",
    "@types/koa": "^2.13.1",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-router": "^7.4.2",
    "@types/node": "^15.0.1",
    "@types/tape": "^4.13.0",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-eslint": "^10.1.0",
    "busboy": "^0.3.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.25.0",
    "eslint-config-alloy": "^4.1.0",
    "eslint-plugin-react": "^7.23.2",
    "koa": "^2.13.1",
    "koa-bodyparser": "^4.3.0",
    "koa-qs": "^3.0.0",
    "koa-router": "^10.0.0",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "tape": "^5.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  }
}
