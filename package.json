{
  "name": "akita",
  "version": "1.0.2",
  "description": "Http client for NodeJs / browser and WeChat, based on fetch()",
  "main": "lib/node.js",
  "browser": "lib/client.js",
  "miniprogram": "lib",
  "types": "index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "cross-env DEBUG=akita* tape test/ts.js",
    "cover": "cross-env DEBUG=akita* nyc node_modules/.bin/tape test/ts.js; nyc report --reporter=html",
    "eslint": "eslint --ext .ts ./",
    "eslint:fix": "eslint --ext .ts --fix ./",
    "prettier": "prettier -l src/** test/**",
    "prettier:fix": "prettier --write -l src/** test/**",
    "fix": "yarn prettier:fix",
    "prepublish": "npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liangxingchen/akita.git"
  },
  "author": {
    "name": "Liang",
    "email": "liang@miaomo.cc",
    "url": "https://github.com/liangxingchen"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/liangxingchen/akita/issues"
  },
  "homepage": "https://github.com/liangxingchen/akita#readme",
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      ".history",
      "coverage",
      "test",
      "lib",
      "**/*.d.ts",
      "*.js"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "dependencies": {
    "debug": "^4.3.4",
    "form-data": "^4.0.0",
    "node-fetch": "^2.6.7",
    "qs": "^6.11.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@types/tape": "^4.13.2",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "babel-eslint": "^10.1.0",
    "busboy": "^0.3.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.27.0",
    "eslint-config-alloy": "^4.7.0",
    "eslint-plugin-react": "^7.31.10",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-qs": "^3.0.0",
    "koa-router": "^12.0.0",
    "nyc": "^15.1.0",
    "prettier": "^2.7.1",
    "tape": "^5.6.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
