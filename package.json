{
  "name": "akita",
  "version": "0.8.2",
  "description": "akita api client",
  "main": "lib/node.js",
  "browser": "lib/client.js",
  "miniprogram": "lib",
  "types": "index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "cross-env DEBUG=akita* tape test/ts.js",
    "cover": "cross-env DEBUG=akita* nyc node_modules/.bin/tape test/ts.js; nyc report --reporter=html",
    "eslint": "eslint --ext .ts,.tsx ./",
    "fix": "eslint --ext .tsx,.ts --fix ./",
    "prepublish": "npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maichong/akita.git"
  },
  "author": {
    "name": "Liang",
    "email": "liang@maichong.it",
    "url": "https://github.com/liangxingchen"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maichong/akita/issues"
  },
  "homepage": "https://github.com/maichong/akita#readme",
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "coverage",
      "test",
      "lib",
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "dependencies": {
    "debug": "^4.1.1",
    "form-data": "^2.3.3",
    "is-buffer": "^2.0.3",
    "node-fetch-unix": "^2.3.0",
    "qs": "^6.7.0"
  },
  "devDependencies": {
    "@types/koa": "^2.0.48",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-router": "^7.0.41",
    "@types/node": "^12.0.8",
    "@types/tape": "^4.2.33",
    "@typescript-eslint/eslint-plugin": "^1.10.2",
    "@typescript-eslint/parser": "^1.10.2",
    "async-busboy": "^0.7.0",
    "babel-eslint": "^10.0.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-alloy": "^2.0.4",
    "eslint-plugin-react": "^7.13.0",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-qs": "^2.0.0",
    "koa-router": "^7.4.0",
    "nyc": "^14.1.1",
    "tape": "^4.10.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.2"
  }
}