{
  "name": "akita",
  "version": "0.10.2",
  "description": "akita api client",
  "main": "lib/node.js",
  "browser": "lib/client.js",
  "miniprogram": "lib",
  "types": "index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "cross-env DEBUG=akita* tape test/ts.js",
    "cover": "cross-env DEBUG=akita* nyc node_modules/.bin/tape test/ts.js; nyc report --reporter=html",
    "eslint": "eslint --ext .ts ./",
    "eslint:fix": "eslint --ext .ts --fix ./",
    "prettier": "prettier -l src/** test/**",
    "prettier:fix": "prettier --write -l src/** test/**",
    "fix": "yarn prettier:fix",
    "prepublish": "npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maichong/akita.git"
  },
  "author": {
    "name": "Liang",
    "email": "liang@maichong.it",
    "url": "https://github.com/liangxingchen"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maichong/akita/issues"
  },
  "homepage": "https://github.com/maichong/akita#readme",
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      ".history",
      "coverage",
      "test",
      "lib",
      "**/*.d.ts",
      "*.js"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "dependencies": {
    "debug": "^4.1.1",
    "form-data": "^3.0.0",
    "is-buffer": "^2.0.4",
    "node-fetch-unix": "^2.3.0",
    "qs": "^6.9.4"
  },
  "devDependencies": {
    "@types/koa": "^2.11.3",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-router": "^7.4.1",
    "@types/node": "^14.0.24",
    "@types/tape": "^4.13.0",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "async-busboy": "0.7.0",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.5.0",
    "eslint-config-alloy": "^3.7.4",
    "eslint-plugin-react": "^7.20.3",
    "koa": "^2.13.0",
    "koa-bodyparser": "^4.3.0",
    "koa-qs": "^3.0.0",
    "koa-router": "^9.1.0",
    "nyc": "^15.1.0",
    "prettier": "^2.0.5",
    "tape": "^5.0.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  }
}
