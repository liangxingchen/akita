{
  "name": "akita",
  "version": "0.6.3",
  "description": "akita api client",
  "main": "lib/node.js",
  "browser": "lib/client.js",
  "miniprogram": "lib",
  "types": "index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "cross-env DEBUG=akita* tape test/ts.js",
    "cover": "cross-env DEBUG=akita* nyc node_modules/.bin/tape test/ts.js; nyc report --reporter=html",
    "eslint": "eslint --ext .ts,.tsx ./",
    "fix": "eslint --ext .tsx,.ts --fix ./",
    "prepublish": "npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maichong/akita.git"
  },
  "author": {
    "name": "Liang",
    "email": "liang@maichong.it",
    "url": "https://github.com/liangxingchen"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maichong/akita/issues"
  },
  "homepage": "https://github.com/maichong/akita#readme",
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "coverage",
      "test",
      "lib",
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "dependencies": {
    "debug": "^4.1.0",
    "form-data": "^2.3.3",
    "is-buffer": "^2.0.3",
    "node-fetch-unix": "^2.1.2-rc.3",
    "qs": "^6.5.2"
  },
  "devDependencies": {
    "@types/koa": "^2.0.46",
    "@types/koa-bodyparser": "^5.0.1",
    "@types/koa-router": "^7.0.35",
    "@types/node": "^10.12.9",
    "@types/tape": "^4.2.32",
    "async-busboy": "^0.7.0",
    "babel-eslint": "^10.0.1",
    "cross-env": "^5.2.0",
    "eslint": "4.19.1",
    "eslint-config-alloy": "^1.4.2",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-typescript": "0.12.0",
    "koa": "^2.6.2",
    "koa-bodyparser": "^4.2.1",
    "koa-qs": "^2.0.0",
    "koa-router": "^7.4.0",
    "nyc": "^13.1.0",
    "tape": "^4.9.1",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.6",
    "typescript-eslint-parser": "^21.0.1"
  }
}
