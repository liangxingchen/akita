[ignore]
.*/lib/.*
.*/coverage/.*
.*/.history/.*
.*/node_modules/.*
.*/flow-declarations/.*

[libs]
node_modules/flow-declarations/akita
node_modules/flow-declarations/is-buffer
node_modules/flow-declarations/node-fetch
node_modules/flow-declarations/debug
node_modules/flow-declarations/depd
